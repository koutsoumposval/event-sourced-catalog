{
    "name": "koutsoumposval/event-sourced-catalog",
    "description": "An event sourced Category-Product catalog",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "7.1.*",
        "prooph/common": "4.1.*",
        "prooph/event-sourcing": "5.2.*",
        "doctrine/collections": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6"
    },
    "autoload": {
        "psr-4": {
            "EventSourcedCatalog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EventSourcedCatalog\\Testing\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
